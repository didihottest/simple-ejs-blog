<%- include("partials/header.ejs") %>
<h1>Compose</h1>
<form action="/compose" method="POST">
    <div class="form-group">
        <div><label for="title">Title</label></div>
        <div><input class="form-control" type="text" name="postTitle" placeholder="Insert Title"></div>
        <div><label for="Post">Post</label></div>
        <div><textarea class="form-control" name="postBody" placeholder="Compose Here" rows="10" cols="100"></textarea>
        </div>

    </div>
    <div><button class="btn btn-primary " type="submit" name="button">Publish</button></div>
</form>
